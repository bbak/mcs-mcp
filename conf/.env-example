# Jira Configuration
JIRA_URL=https://your-domain.atlassian.net

# Preferred: Jira Personal Access Token (PAT)
# JIRA_TOKEN=

# Fallback: Session Cookies
JIRA_XSRF_TOKEN=
JIRA_SESSION_ID=
JIRA_REMEMBERME_COOKIE=
JIRA_GCILB=
JIRA_GCLB=

# Performance
JIRA_REQUEST_DELAY_SECONDS=10

# App Configuration
VERBOSE=false

# Optional base folder for logs and cache
# If empty or unset, defaults to the path of the binary.
# DATA_PATH=
